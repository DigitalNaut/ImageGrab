{
  "cSpell.words": [
    "classname",
    "crxjs",
    "cssnano",
    "divs",
    "newtab",
    "Srcs",
    "tailwindcss",
    "typecheck",
    "webextension",
    "webextensions"
  ],
  "tailwindCSS.experimental.classRegex": [
    ["Style \\=([^;]*);", "'([^']*)'"],
    ["Style \\=([^;]*);", "\"([^\"]*)\""],
    ["Style \\=([^;]*);", "\\`([^\\`]*)\\`"]
  ],
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "editor.quickSuggestions": {
    "strings": "on"
  }
}
